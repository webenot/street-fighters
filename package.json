    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^2.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-autoprefixer": "^5.4.0",
    "express-handlebars": "^3.1.0",
    "express-messages": "^1.0.1",
    "express-session": "^1.16.1",
