{
  "name": "street-fighters",
  "version": "0.4.0",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production webpack -p && cross-env NODE_ENV=production NODE_PATH=. node ./bin/www",
    "dev": "cross-env NODE_ENV=development webpack --debug --display-error-details && cross-env NODE_ENV=development NODE_PATH=. nodemon ./bin/www",
    "devServer": "cross-env NODE_ENV=development NODE_PATH=. nodemon ./bin/www",
    "devFront": "cross-env NODE_ENV=development webpack-dev-server --debug --display-error-details",
    "devFrontWatch": "cross-env NODE_ENV=development webpack --watch --debug --display-error-details",
    "startFront": "cross-env NODE_ENV=production webpack-dev-server -p",
    "buildFront": "cross-env NODE_ENV=production webpack -p"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^2.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-autoprefixer": "^5.4.0",
    "express-handlebars": "^3.1.0",
    "express-messages": "^1.0.1",
    "express-session": "^1.17.0",
    "fs-extra": "^8.1.0",
    "http-errors": "~1.6.3",
    "joi": "^14.3.1",
    "mongoose": "^5.7.14",
    "morgan": "~1.9.1",
    "nconf": "^0.10.0",
    "node-bourbon": "^4.2.8",
    "node-fetch": "^2.6.0",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "postcss-middleware": "^1.1.4",
    "pug": "^2.0.4",
    "rotating-file-stream": "^1.4.6",
    "shortid": "^2.2.15",
    "tar": "^4.4.13",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0",
    "webpack-node-externals": "^1.7.2"
  }
